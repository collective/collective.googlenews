<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="collective.googlenews">
  <configlet
      title="Google News Settings"
      action_id="collective.googlenews.settings"
      appId="collective.googlenews"
      category="Products"
      condition_expr=""
      url_expr="string:${portal_url}/@@googlenews-controlpanel"
      icon_expr="string:${portal_url}/++resource++googlenews.png"
      visible="True"
      i18n:attributes="title">
    <permission>Manage portal</permission>
  </configlet>
</object>
